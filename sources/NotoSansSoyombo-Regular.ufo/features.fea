} half_marks;


lookup add_tail {
	# Long tail for vowel and consonant marks
	sub @sub0' lookup add_tail_inside aFinal-soyo [@halfBottomMarks0 @halfBottomMarks1];
	sub @sub0' lookup add_tail_inside [@halfBottomMarks0 @halfBottomMarks1];

	# Normal tail
	sub gFinal-soyo' @halfBottomMarks0 by gFinal-soyo tail-soyo;
	sub kFinal-soyo' @halfBottomMarks0 by kFinal-soyo tail-soyo;
	sub ngFinal-soyo' @halfBottomMarks0 by ngFinal-soyo tail-soyo;
	sub dFinal-soyo' @halfBottomMarks0 by dFinal-soyo tail-soyo;
	sub nFinal-soyo' @halfBottomMarks0 by nFinal-soyo tail-soyo;
	sub bFinal-soyo' @halfBottomMarks0 by bFinal-soyo tail-soyo;
	sub mFinal-soyo' @halfBottomMarks0 by mFinal-soyo tail-soyo;
	sub rFinal-soyo' @halfBottomMarks0 by rFinal-soyo tail-soyo;
	sub lFinal-soyo' @halfBottomMarks0 by lFinal-soyo tail-soyo;
	sub shFinal-soyo' @halfBottomMarks0 by shFinal-soyo tail-soyo;
	sub sFinal-soyo' @halfBottomMarks0 by sFinal-soyo tail-soyo;
