feature psts {
# Post-base substitution



@initBreak = [@halfBottomMarks0 @subLetters];

@add_head_matters = [@preHead @baseLetters @halfBottomMarks0];

lookup add_head {
	# Decompose a letter into head and sub parts
	
	# if there's top decoration before
	sub @preHead @baseLetters' lookup add_head_inside;

	# if it's initial of a multi-consonant syllable
	sub @baseLetters' lookup add_head_inside @sub0;

	# if it's followed by vowel marks
	sub @baseLetters' lookup add_head_inside @halfBottomMarks0;
} add_head;

lookup open_head {
	# Some letters need to have stylistically different (open) head
	sub head-soyo' [kaSub-soyo khaSub-soyo gaSub-soyo ghaSub-soyo ngaSub-soyo haSub-soyo kssaSub-soyo] by head-soyo.open;
} open_head;


lookup remove_dottedCircle {
# Between final consonant and a vowel sign, a dottedCircle seems to be added by certain layout engines after ccmp.
# This lookup removes that.
	sub dottedCircle' gemination-soyo' by gemination-soyo;
	sub gemination-soyo' dottedCircle' by gemination-soyo;
	sub dottedCircle' gFinal-soyo' by gFinal-soyo;
	sub dottedCircle' kFinal-soyo' by kFinal-soyo;
	sub dottedCircle' ngFinal-soyo' by ngFinal-soyo;
	sub dottedCircle' dFinal-soyo' by dFinal-soyo;
	sub dottedCircle' nFinal-soyo' by nFinal-soyo;
	sub dottedCircle' bFinal-soyo' by bFinal-soyo;
	sub dottedCircle' mFinal-soyo' by mFinal-soyo;
	sub dottedCircle' rFinal-soyo' by rFinal-soyo;
	sub dottedCircle' lFinal-soyo' by lFinal-soyo;
	sub dottedCircle' shFinal-soyo' by shFinal-soyo;
	sub dottedCircle' sFinal-soyo' by sFinal-soyo;
	sub dottedCircle' aFinal-soyo' by aFinal-soyo;
	sub dottedCircle' iMatra-soyo' by iMatra-soyo;
	sub dottedCircle' ueMatra-soyo' by ueMatra-soyo;
	sub dottedCircle' uMatra-soyo' by uMatra-soyo;
	sub dottedCircle' eMatra-soyo' by eMatra-soyo;
	sub dottedCircle' oMatra-soyo' by oMatra-soyo;
	sub dottedCircle' oeMatra-soyo' by oeMatra-soyo;
	sub dottedCircle' rVocalicMatra-soyo' by rVocalicMatra-soyo;
	sub dottedCircle' lVocalicMatra-soyo' by lVocalicMatra-soyo;
	sub dottedCircle' vowelLength-soyo' by vowelLength-soyo;
	sub dottedCircle' aiMatra-soyo' by aiMatra-soyo;
	sub dottedCircle' auMatra-soyo' by auMatra-soyo;
	sub dottedCircle' visagra-soyo' by visagra-soyo;
	sub dottedCircle' anusvara-soyo' by anusvara-soyo;

	sub dottedCircle @halfBottomMarks0' by @halfBottomMarks0;
} remove_dottedCircle;


lookup add_head2 {
# adds tail for aFinal cases that couldn't be done in ccmp due to the dottedCircle issue.
	sub @baseLetters' lookup add_head_inside aFinal-soyo @halfBottomMarks0;
