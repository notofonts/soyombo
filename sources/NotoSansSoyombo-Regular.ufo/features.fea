lookup add_joiner_preConsonant2 {
	sub @pushing_preConsonant head-soyo' [aSub-soyo phaSub-soyo bhaSub-soyo maSub-soyo zhaSub-soyo zaSub-soyo yaSub-soyo raSub-soyo laSub-soyo] by head-soyo joiner-soyo.right;
} add_joiner_preConsonant2;

@subLetters_headSub_bottomMarks = [headSub-soyo headSub-soyo.open @sub0 @halfBottomMarks0 @halfBottomMarks1];

lookup condense3_ini1 { # condense letters, 3 or more consonant cases, up to 10
	lookupflag UseMarkFilteringSet @subLetters_head;
	sub [head-soyo headSub-soyo head-soyo.open headSub-soyo.open] @sub0' @sub0 @sub0 by @ini3;
} condense3_ini1;


lookup condense3_rest {
	lookupflag UseMarkFilteringSet @subLetters_head;
	sub [@ini3 @sub3] @sub0' by @sub3;
	sub @sub3 @sub0' by @sub3;
	sub @sub3 @sub0' by @sub3;
	sub @sub3 @sub0' by @sub3;
	sub @sub3 @sub0' by @sub3;
	sub @sub3 @sub0' by @sub3;
	sub @sub3 @sub0' by @sub3;
	sub @sub3 @sub0' by @sub3;
	sub @sub3 @sub0' by @sub3;
} condense3_rest;


lookup condense3_ini2 { # 2 consonants + prehead and final consonant cases
	lookupflag UseMarkFilteringSet @subLetters_headSub_bottomMarks;
	sub [headSub-soyo headSub-soyo.open] @sub0' @sub0 [@halfBottomMarks0 @halfBottomMarks1] by @ini3;
# 	sub @ini3 @sub0' by @sub3;
} condense3_ini2;


lookup condense3_ini2_rest {
	lookupflag UseMarkFilteringSet @subLetters_head;
	sub @ini3 @sub0' by @sub3;
} condense3_ini2_rest;


lookup condense2 { # condense letters, 2 consonant cases
	lookupflag UseMarkFilteringSet @subLetters_head;
	sub [head-soyo headSub-soyo head-soyo.open headSub-soyo.open] @sub0' @sub0 by @ini2;
	sub [@ini2 @sub2] @sub0' by @sub2;
} condense2;

lookup condense1 { # condense letters, 1 consonant + prehead and final consonant
	lookupflag UseMarkFilteringSet @subLetters_headSub_bottomMarks;
	sub [headSub-soyo headSub-soyo.open] @sub0' [@halfBottomMarks0 @halfBottomMarks1] by @ini2;
} condense1;

lookup add_joiner_preConsonant3 {
	lookupflag 0;
	sub @pushing_preConsonant head-soyo' [taSub-soyo.ini3 thaSub-soyo.ini3 paSub-soyo.ini3 aaSub-soyo.ini3 vaSub-soyo.ini3 shaSub-soyo.ini3 ssaSub-soyo.ini3] by head-soyo joiner-soyo.right.tight;
	sub @pushing_preConsonant head-soyo.open' [haSub-soyo.ini2 haSub-soyo.ini3 ] by head-soyo.open joiner-soyo.blank;
	sub @pushing_preConsonant head-soyo' [ddhaSub-soyo.ini3 dhaSub-soyo.ini3] by head-soyo joiner-soyo.both.tight;
	sub @pushing_preConsonant head-soyo.open' [kaSub-soyo.ini3 khaSub-soyo.ini3 gaSub-soyo.ini3 ghaSub-soyo.ini3 ngaSub-soyo.ini3 kssaSub-soyo.ini3 haSub-soyo.ini2 haSub-soyo.ini3 ] by head-soyo.open joiner-soyo.both.tight;
} add_joiner_preConsonant3;

lookup lower_diphthongs {
	lookupflag UseMarkFilteringSet @diphthongs;
	sub [ka-soyo kha-soyo ga-soyo gha-soyo nga-soyo ha-soyo kssa-soyo head-soyo.open] [aiMatra-soyo auMatra-soyo]' by [aiMatra-soyo.open auMatra-soyo.open];
