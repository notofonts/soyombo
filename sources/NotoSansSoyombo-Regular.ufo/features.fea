feature dist {
# essentially kerning, but I do not want it disabled

@dist_joiners =[joiner-soyo.blank joiner-soyo.mid joiner-soyo.both.nonSpacing joiner-soyo.right joiner-soyo.both.tight joiner-soyo.right.tight];

# preConsonants and decomposed letters
lookup precons_decomp {
	ignore pos shaInitial-soyo' @head [@lsb50Sub @lsb40Sub];
	pos @preConsonants' <50 0 0 0> @head @dist_joiners @lsb50Sub;
	pos @preConsonants' <40 0 0 0> @head @dist_joiners @lsb40Sub;
	pos @preConsonants' <10 0 0 0> @head @dist_joiners @lsb10Sub;
	pos @preConsonants' <-10 0 0 0> @head @dist_joiners @lsb-10Sub;
	pos @preConsonants' <-20 0 0 0> @head @dist_joiners @lsb-20Sub;
	pos @preConsonants' <-30 0 0 0> @head @dist_joiners @lsb-30Sub;
	pos @preConsonants' <-40 0 0 0> @head @dist_joiners @lsb-40Sub;
	pos @preConsonants' <-50 0 0 0> @head @dist_joiners @lsb-50Sub;

	pos @preConsonants' <50 0 0 0> @head @lsb50Sub;
	pos @preConsonants' <40 0 0 0> @head @lsb40Sub;
	pos @preConsonants' <10 0 0 0> @head @lsb10Sub;
	pos @preConsonants' <-10 0 0 0> @head @lsb-10Sub;
	pos @preConsonants' <-20 0 0 0> @head @lsb-20Sub;
	pos @preConsonants' <-30 0 0 0> @head @lsb-30Sub;
	pos @preConsonants' <-40 0 0 0> @head @lsb-40Sub;
	pos @preConsonants' <-50 0 0 0> @head @lsb-50Sub;
} precons_decomp;


# preConsonants and precomposed letters
lookup precons_precomp {
	ignore pos shaInitial-soyo' [@lsb50Letter @lsb40Letter];
	pos @preConsonants' <50 0 0 0> @lsb50Letter;
	pos @preConsonants' <40 0 0 0> @lsb40Letter;
	pos @preConsonants' <10 0 0 0> @lsb10Letter;
	pos @preConsonants' <-10 0 0 0> @lsb-10Letter;
	pos @preConsonants' <-20 0 0 0> @lsb-20Letter;
	pos @preConsonants' <-30 0 0 0> @lsb-30Letter;
	pos @preConsonants' <-40 0 0 0> @lsb-40Letter;
	pos @preConsonants' <-50 0 0 0> @lsb-50Letter;
} precons_precomp;


# preConsonant sha adjustment for narrow glyphs with centre stem.
lookup preSha_adjust {
	pos shaInitial-soyo' <30 0 0 0> @head [@lsb50Sub @lsb40Sub];
	pos shaInitial-soyo' <30 0 0 0> [@lsb50Letter @lsb40Letter];
	pos shaInitial-soyo' <0 0 20 0> @narrowCentreStemLetter;
	pos shaInitial-soyo' <0 0 20 0> @head @narrowCentreStemSub;
} preSha_adjust;


# head and decomposed letters
lookup prehead_decomp {
	pos @preHead' <50 0 50 0> @headSub @lsb50Sub;
	pos @preHead' <40 0 40 0> @headSub @lsb40Sub;
	pos @preHead' <10 0 10 0> @headSub @lsb10Sub;
	pos @preHead' <-10 0 -10 0> @headSub @lsb-10Sub;
	pos @preHead' <-20 0 -20 0> @headSub @lsb-20Sub;
	pos @preHead' <-30 0 -30 0> @headSub @lsb-30Sub;
	pos @preHead' <-40 0 -40 0> @headSub @lsb-40Sub;
	pos @preHead' <-50 0 -50 0> @headSub @lsb-50Sub;
} prehead_decomp;


# head and decomposed letters
lookup head_decomp {
	pos @head' <50 0 50 0> @dist_joiners @lsb50Sub;
	pos @head' <40 0 40 0> @dist_joiners @lsb40Sub;
	pos @head' <10 0 10 0> @dist_joiners @lsb10Sub;
	pos @head' <-10 0 -10 0> @dist_joiners @lsb-10Sub;
	pos @head' <-20 0 -20 0> @dist_joiners @lsb-20Sub;
	pos @head' <-30 0 -30 0> @dist_joiners @lsb-30Sub;
	pos @head' <-40 0 -40 0> @dist_joiners @lsb-40Sub;
	pos @head' <-50 0 -50 0> @dist_joiners @lsb-50Sub;

	pos @head' <50 0 50 0> @lsb50Sub;
	pos @head' <40 0 40 0> @lsb40Sub;
	pos @head' <10 0 10 0> @lsb10Sub;
	pos @head' <-10 0 -10 0> @lsb-10Sub;
	pos @head' <-20 0 -20 0> @lsb-20Sub;
	pos @head' <-30 0 -30 0> @lsb-30Sub;
	pos @head' <-40 0 -40 0> @lsb-40Sub;
	pos @head' <-50 0 -50 0> @lsb-50Sub;
} head_decomp;

