lookup add_joiner2 { # adjust joiner stem shape
	# both stem cases
	sub @bothStemBefore joiner-soyo.right' @bothStemAfter by joiner-soyo.both.nonSpacing;
	# mid stem spaced cases
	sub @midStemBefore joiner-soyo.right' @midStemAfter by joiner-soyo.mid;
	# blank stem cases
	sub @sub0 joiner-soyo.right' @midStemAfter by joiner-soyo.blank;
} add_joiner2;


lookup unspace_joiner {
	sub @touchCentreBefore joiner-soyo.right' @touchOpenAfter by joiner-soyo.right.tight;
